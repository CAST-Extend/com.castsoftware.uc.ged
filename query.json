                "name": "DB_NoCallers",
                "query": [
                    "MATCH (o:Object:{config.aip_name})",
					"WHERE o.Type contains 'Procedure' AND NOT (:Object)-[]->(o)",
					"return DISTINCT o.FullName"
                ],
                "formating": {
