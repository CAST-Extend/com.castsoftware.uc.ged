        "tabs": [
            {
                "name": "API",
                "query": [
                    "MATCH (m:Module:{config.aip_name})-[:Contains]->(o1:Object)-[]-(o2:Object)<-[]-(l:Level5) ",
                    "WHERE l.Name CONTAINS 'API' ",
                    "RETURN DISTINCT m.Name as `Repository Name`, COLLECT(DISTINCT l.Name) as `API Name` "
                ],
                "formating": {
